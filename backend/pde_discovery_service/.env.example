# PostgreSQL database connection string for PDE Discovery Service
DATABASE_URL="postgresql+psycopg2://user:password@localhost:5435/pde_discovery_db"

# MinIO/S3 Object Storage Configuration for PDE discovery results and logs
MINIO_ENDPOINT="localhost:9000"
MINIO_ACCESS_KEY="minioadmin"
MINIO_SECRET_KEY="minioadmin"
MINIO_SECURE=True
MINIO_BUCKET_NAME="pde-discovery-results"

# Job Orchestration Service Database URL (for worker to update job status)
JOB_ORCHESTRATION_DATABASE_URL="postgresql+psycopg2://user:password@localhost:5434/job_db"

# Set the environment. 'development' allows auto-migrations.
ENVIRONMENT="development"
