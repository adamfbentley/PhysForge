# PostgreSQL database connection string for Active Experiment Service
DATABASE_URL="postgresql+psycopg2://user:password@localhost:5436/active_exp_db"

# MinIO/S3 Object Storage Configuration for active experiment results and logs
MINIO_ENDPOINT="localhost:9000"
MINIO_ACCESS_KEY="minioadmin"
MINIO_SECRET_KEY="minioadmin"
MINIO_SECURE=True
MINIO_BUCKET_NAME="active-experiment-results"

# Job Orchestration Service Database URL (for worker to update job status)
JOB_ORCHESTRATION_DATABASE_URL="postgresql+psycopg2://user:password@localhost:5434/job_db"

# PDE Discovery Service URL (for fetching PDE results if needed, though MinIO paths are used)
PDE_DISCOVERY_SERVICE_URL="http://localhost:8004"

# Set the environment. 'development' allows auto-migrations.
ENVIRONMENT="development"
