# Environment Variables for PhysForge Frontend
# Copy this file to .env.local and update the values as needed

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_API_GATEWAY_URL=http://localhost:8080

# Application Configuration
REACT_APP_NAME=PhysForge
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false

# Third-party Services (optional)
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=

# Development
REACT_APP_DEBUG=true